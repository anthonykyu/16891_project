<?xml version="1.0"?>
<launch>
  	<!-- <arg name="trial" default="$(find mamp_planning)/src/agents.txt"/> -->
  	<arg name="test" default="world_mobile_1_test_0.txt"/>
	<param name="test" value="$(arg test)"/>

	<param name="mobile_robot" command=" cat $(find panda_multiple_arms)/robot_description/mobile_robot.urdf" />
	<param name="walls" command=" cat $(find panda_multiple_arms)/robot_description/walls.urdf" />

	<!-- With shelves -->
	<param name="world_mobile_1" command=" cat $(find panda_multiple_arms)/robot_description/world_mobile_1.urdf" />
	<param name="world_mobile_4" command=" cat $(find panda_multiple_arms)/robot_description/world_mobile_4.urdf" />
	<param name="world_mobile_9" command=" cat $(find panda_multiple_arms)/robot_description/world_mobile_9.urdf" />

	<!-- Without Shelves -->
	<param name="world_no_shelves_mobile_1" command=" cat $(find panda_multiple_arms)/robot_description/world_no_shelves_mobile_1.urdf" />
	<param name="world_no_shelves_mobile_4" command=" cat $(find panda_multiple_arms)/robot_description/world_no_shelves_mobile_4.urdf" />
	<param name="world_no_shelves_mobile_9" command=" cat $(find panda_multiple_arms)/robot_description/world_no_shelves_mobile_9.urdf" />

	<node pkg="mamp_planning" type="mamp_planning_node" name="mamp_planning_node" output="screen"/>
	

	  <!--  **************** Visualisation ****************  -->
	<!-- <node name="rviz" pkg="rviz" type="rviz" args="-d $(find steer_bot_viz)/rviz/steer_bot_planning.rviz"/> -->

</launch>